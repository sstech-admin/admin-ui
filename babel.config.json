{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "env": {
    "development": {
      "presets": ["@babel/preset-react"]
    },
    "production": {
      "presets": ["@babel/preset-react"]
    }
  },
  "plugins": ["@babel/plugin-transform-private-property-in-object"]
}
